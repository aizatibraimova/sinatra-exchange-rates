<h1>Convert <%= @original_currency %></h1>

<div>
  Go <a href="/">back</a>. 
</div>

<ul>
  <% @symbols.each do |symbol, data | %>
    <li>
      <a href="/<%= @original_currency %>/<%= symbol %>">
        Convert 1 <%= @original_currency %> to <%= data["code"] %>...
      </a>
    </li>
  <% end %>
</ul>
